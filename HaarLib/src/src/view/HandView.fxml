<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="apHandViewWindow" prefHeight="680.0" prefWidth="831.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.controller.HandViewController">
    <children>
        <BorderPane layoutX="6.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <center>
                <ImageView fx:id="layarMain" fitHeight="532.0" fitWidth="596.0" onMouseClicked="#getPoint" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
            </center>
            <left>
                <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                    <children>
                  <ImageView fx:id="layarHand" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#bwToMn" pickOnBounds="true" preserveRatio="true" />
                        <ImageView fx:id="layarBW" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#bwToMn" pickOnBounds="true" preserveRatio="true" />
                        <ImageView fx:id="layarEdge" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#edgeToMn" pickOnBounds="true" preserveRatio="true" />
                  <ComboBox fx:id="cmbDescriptor" prefWidth="150.0" promptText="Descriptor" />
                        <Button fx:id="btnStartCamera" mnemonicParsing="false" onAction="#startCameraOnClick" text="Start Camera" />
                        <TextField fx:id="txtMainFramePoint" promptText="Main Frame Point" />
                        <Button mnemonicParsing="false" onAction="#browseClassifierOnClick" text="Browse" />
                        <Button fx:id="btnUpdateCamera" mnemonicParsing="false" onAction="#getClassifierOnClick" text="Get Classifier" />
                  <ComboBox fx:id="cmbClassifier" prefWidth="150.0" promptText="Classifier" />
                        <TextField fx:id="txtBoxWidth" promptText="width" />
                  <TextField fx:id="txtPredictedResult" promptText="Predicted Result" />
                    </children>
                </VBox>
            </left>
        </BorderPane>
    </children>
</AnchorPane>
